{
  "expo": {
    "name": "Miss Bunny Penny",
    "slug": "react_native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/penny_splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#333"
    },
    "jsEngine": "hermes",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#333"
      },
      "package": "live.fivezero.fnitemshop",
    },
    "web": {
      "favicon": "./assets/icon.png"
    }
  }
}
